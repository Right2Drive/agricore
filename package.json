{
  "name": "agricore",
  "version": "1.0.0",
  "description": "The Agricore",
  "main": "src/index.ts",
  "author": "Agriculture Capstone Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 8.9.0, < 9.0.0",
    "npm": ">= 5.5.1",
    "yarn": ">= 1.3.0"
  },
  "scripts": {
    "start": "npm run build:dev && npm run concurrently -- \"npm run watch\" \"npm run serve:dev\"",
    "start:prod": "npm run build:prod && npm run serve:prod",
    "debug": "npm run build:dev && npm run concurrently -- \"npm run watch\" \"npm run serve:debug\"",
    "clean": "rimraf dist/*",
    "doc": "node_modules/.bin/apidoc -i ./ -o apidoc/ -e node_modules",
    "build": "npm run build:prod",
    "build:dev": "npm run webpack -- --env=dev",
    "build:prod": "npm run webpack -- --env=prod",
    "watch": "node_modules/.bin/cross-env NODE_ENV=notify npm run build:dev -- --watch",
    "serve:dev": "node_modules/.bin/nodemon dist/app.js",
    "serve:debug": "npm run serve:dev -- --inspect",
    "serve:prod": "node dist/app.js",
    "webpack": "node_modules/.bin/webpack",
    "test": "npm run lint && npm run coverage -- npm run mocha-webpack -- \"test/**/*.test.ts\"",
    "test:unit": "npm run mocha-webpack -- \"test/unit/**/*.test.ts\"",
    "test:inte": "npm run mocha-webpack -- \"test/integration/**/*.test.ts\"",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:fix": "npm run lint:src:fix && npm run lint:test:fix",
    "lint:test": "npm run tslint -- \"test/**/*.ts\"",
    "lint:test:fix": "npm run lint:test -- --fix",
    "lint:src": "npm run tslint -- \"src/**/*.ts\"",
    "lint:src:fix": "npm run lint:src -- --fix",
    "mocha-webpack": "node_modules/.bin/mocha-webpack --require test/setup.ts --include test/bundledSetup.ts --webpack-env test --webpack-config webpack.config.js",
    "coverage": "node_modules/.bin/cross-env COVERAGE=true node_modules/.bin/nyc",
    "tslint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "concurrently": "node_modules/.bin/concurrently -k -p \"[{name}]\" -n \"Webpack,Node\" -c \"cyan.bold,green.bold\""
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "body-parser": "1.18.0",
    "compression": "1.7.0",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "express": "4.15.4",
    "express-jwt": "5.3.0",
    "jsonwebtoken": "8.1.0",
    "knex": "0.13.0",
    "mocha": "3.5.2",
    "morgan": "1.8.2",
    "mysql": "2.14.1",
    "pg": "7.4.0",
    "posix-character-classes": "^1.0.0",
    "resolv": "^1.0.0",
    "source-map-support": "^0.5.3",
    "tslint-loader": "3.5.3",
    "typescript": "2.5.2",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "1.0.0",
    "@types/body-parser": "1.16.5",
    "@types/chai": "4.0.5",
    "@types/compression": "0.0.34",
    "@types/cors": "2.8.1",
    "@types/dotenv": "4.0.1",
    "@types/express": "4.0.37",
    "@types/express-jwt": "0.0.37",
    "@types/jsonwebtoken": "7.2.3",
    "@types/knex": "0.0.61",
    "@types/mocha": "2.2.43",
    "@types/morgan": "1.7.32",
    "@types/node": "8.0.28",
    "@types/sinon": "4.0.0",
    "@types/webpack-env": "1.13.1",
    "@types/winston": "2.3.5",
    "apidoc": "0.17.6",
    "awesome-typescript-loader": "3.2.3",
    "chai": "4.1.2",
    "concurrently": "3.5.0",
    "cross-env": "5.1.1",
    "friendly-errors-webpack-plugin": "1.6.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "mocha-webpack": "1.0.1",
    "nodemon": "1.12.0",
    "nyc": "11.3.0",
    "rimraf": "2.6.1",
    "sinon": "4.1.2",
    "source-map-loader": "0.2.1",
    "tslint": "5.7.0",
    "tslint-config-airbnb": "5.2.1",
    "webpack": "3.5.6",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0",
    "webpack-notifier": "1.5.0"
  },
  "apidoc": {
    "name": "The Agricore",
    "version": "0.0.1",
    "description": "Robust REST API Boilerplate",
    "title": "CoreAPI",
    "url": ""
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./test/coverage",
    "instrument": false,
    "sourceMap": false
  }
}
